	pages = {1817--1826},
	file = {ACM Full Text PDF:/Users/andrew/Library/Application Support/Zotero/Profiles/g1y3llo0.default/zotero/storage/TCRTKAGH/Fourney et al. - 2011 - Characterizing the Usability of Interactive Applic.pdf:application/pdf}
